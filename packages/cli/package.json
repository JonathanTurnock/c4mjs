{
  "name": "@c4mjs/cli",
  "version": "0.1.0",
  "private": true,
  "description": "C4Model JS Command Line Utility",
  "keywords": [
    "c4mjs",
    "cli"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "c4mjs": "./dist/index.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run generate:SourceWorkspaceDto && npm run generate:WorkspaceDto",
    "build": "tsc --outDir dist",
    "build:watch": "tsc-watch --outDir dist",
    "test": "jest --passWithNoTests --coverage src",
    "generate:SourceWorkspaceDto": "typescript-json-schema 'src/dtos/*.dto.ts' 'SourceWorkspaceDto' > 'src/source-workspace.schema.json' --required --noExtraProps --refs --topRef",
    "generate:WorkspaceDto": "typescript-json-schema 'src/dtos/*.dto.ts' 'WorkspaceDto' > 'src/workspace.schema.json' --required --noExtraProps --refs --topRef"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.9",
    "ajv": "^8.11.0",
    "commander": "^9.4.0",
    "debug": "^4.3.4",
    "eslint": "8.22.0",
    "handlebars": "^4.7.7",
    "inquirer": "8.2.4",
    "lodash": "^4.17.21",
    "typescript-json-schema": "^0.54.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.184"
  }
}
