{
  "name": "@c4mjs/cli",
  "version": "1.1.8",
  "license": "MIT",
  "description": "C4Model JS Command Line Utility",
  "keywords": [
    "c4mjs",
    "cli"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "c4mjs": "./dist/index.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "typescript-json-schema 'src/dtos/*.dto.ts' 'SourceWorkspaceDto' > 'src/source-workspace.schema.json' --required --noExtraProps --refs --topRef --id C4MJS",
    "build": "tsc --outDir dist",
    "postbuild": "rimraf ../../docs/source-workspace* && node ./scripts/prepare-docs-schema.js && jsonschema2md -d .schema && rimraf out/README.md && mv out/* ../../docs",
    "build:watch": "tsc-watch --outDir dist",
    "test": "jest --passWithNoTests --coverage src"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.9",
    "@c4mjs/validation": "1.1.8",
    "@c4mjs/workspace": "1.1.8",
    "aigle": "^1.14.1",
    "commander": "^9.4.0",
    "debug": "^4.3.4",
    "eslint": "8.22.0",
    "handlebars": "^4.7.7",
    "inquirer": "8.2.4",
    "lodash": "^4.17.21",
    "sqlite3": "^5.0.11",
    "typescript-json-schema": "^0.54.0"
  },
  "devDependencies": {
    "@adobe/jsonschema2md": "^7.1.1",
    "@types/lodash": "^4.14.184",
    "@types/sqlite3": "^3.1.8"
  },
  "gitHead": "ebdc771a7840992ef7720e5ccc17a5a91ac7e3af"
}
