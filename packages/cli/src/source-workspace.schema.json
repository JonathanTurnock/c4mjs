{
    "$id": "C4MJS",
    "$ref": "C4MJS#/definitions/SourceWorkspaceDto",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "SourceComponentDto": {
            "additionalProperties": false,
            "description": "Component inside a Systems Container, represents a software component",
            "properties": {
                "deps": {
                    "description": "Multiline string representing the entities dependecies, seperate dependecies with a new line. 'this' can be used to reference the current entity",
                    "examples": [
                        "this -> emailSystem : Sends Emails using"
                    ],
                    "title": "Dependencies",
                    "type": "string"
                },
                "desc": {
                    "description": "Description to be used to convey information about the entity",
                    "title": "Description",
                    "type": "string"
                },
                "external": {
                    "description": "Is the entity external or not",
                    "examples": [
                        "true",
                        "false"
                    ],
                    "title": "External",
                    "type": "boolean"
                },
                "id": {
                    "description": "ID, must be camel case",
                    "examples": [
                        "bigBankPlc",
                        "mainframeBankingSystem",
                        "emailSystem",
                        "webApplication"
                    ],
                    "pattern": "^([a-zA-Z0-9])*$",
                    "title": "ID",
                    "type": "string"
                },
                "name": {
                    "description": "Workspace name",
                    "examples": [
                        "Big Bank PLC",
                        "Mainframe Banking System",
                        "Email System",
                        "Web Application"
                    ],
                    "title": "Name",
                    "type": "string"
                },
                "tags": {
                    "description": "Tags that can be attached to an entity",
                    "examples": [
                        "cba",
                        "critical"
                    ],
                    "items": {
                        "type": "string"
                    },
                    "title": "Tags",
                    "type": "array"
                },
                "tech": {
                    "description": "What technology does the entity use",
                    "examples": [
                        "React App",
                        "Spring MVC Controller"
                    ],
                    "title": "Tech",
                    "type": "string"
                }
            },
            "required": [
                "id",
                "name"
            ],
            "title": "Component",
            "type": "object"
        },
